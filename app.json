{
  "expo": {
    "name": "Mudaheranwa",
    "slug": "Mudaheranwa-Artist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mudaa-logo-icon.png",
    "scheme": "mudaheranwaartist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show it on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mudaa-logo-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "edgeToEdgeEnabled": true,
      "package": "com.florien2208.MudaheranwaArtist"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/mudaa-logo-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/muda-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#72b7e9"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4aa104da-5732-4172-ba26-ccc126d8d9e5"
      }
    }
  }
}
